{
  "name": "ProjectName",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "clean": "npm start  --reset-cache",
    "ios": "react-native run-ios --simulator='iPhone 16'",
    "linter": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "type-check": "tsc --noEmit",
    "lint-and-type-check": "npm run type-check && npm run linter",
    "prepare": "husky install",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "sort-package-json": "npx sort-package-json",
    "start": "npx react-native start ",
    "test": "jest",
    "generate": "plop && prettier --write 'src/**/*.{js,jsx,ts,tsx}' && eslint --fix 'src/**/index.ts'",
    "postinstall": "patch-package",
    "assets": "npx react-native-asset"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
